   
<h1>Listing users</h1>
<%= link_to 'Back', admin_path %>

<table>
  <tr>
  	<th>id</th>
    <th>Username</th>
    <th>Email</th>
    <th>First name</th>
    <th>Last name</th>
    <th>About</th>
    <th>Age</th>
    <th>Sex</th>
    <th>Zip code</th>
    <th>Uid</th>
    <th>Provider</th>
	<th>Time Zone</th>
	<th>Created At</th>
	<th>Updated At</th>
    <th></th>
  </tr>
	
<% @users.each do |user| %>
  <tr>
  	<td><%= user.id %></td>
    <td><%= user.username %></td>
    <td><%= user.email %></td>
    <td><%= user.first_name %></td>
    <td><%= user.last_name %></td>
    <td><%= user.about %></td>
    <td><%= user.age %></td>
    <td><%= user.sex %></td>
    <td><%= user.zip_code %></td>
	<td><%= user.uid %></td>
	<td><%= user.provider %></td>
	<td><%= user.time_zone %></td>
	<td><%= user.created_at %></td>
	<td><%= user.updated_at %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
	</tr>
<% end %>
</table>

<br />

